{
	"defaultSeverity": "warn",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"curly": false,
		"no-any": false,
		"no-empty": false,
		"no-console": false,
		"class-name": false,
		"array-type": false,
		"no-bitwise": false,
		"prefer-const": true,
		"prefer-for-of": false,
		"no-var-keyword": true,
		"interface-name": false,
		"trailing-comma": false,
		"ordered-imports": false,
		"max-classes-per-file": false,
		"only-arrow-functions": false,
		"no-trailing-whitespace": false,
		"object-literal-sort-keys": false,
		"object-literal-shorthand": false,
		"max-line-length": [
			true,
			150
		],
		"semicolon": [
			true,
			"always"
		],
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs",
			4
		]
	}
}